;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Wild Jam 80"
config/version="0.0.1"
run/main_scene="res://scenes/screens/main_menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Scenes="*res://autoloads/scenes.gd"
SignalBus="*res://autoloads/SignalBus.gd"
SaveSystem="*res://addons/save_system/save_system.gd"
Utility="*res://autoloads/Utility.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
g="*res://autoloads/global.gd"
Onstart="*res://autoloads/onstart.gd"
SceneManager="*res://addons/awesome_scene_manager/autoloads/SceneManager.tscn"
Cursor="*res://addons/awesome_custom_cursor/Autoloads/Cursor.tscn"
Console="*res://addons/console/console.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=320
window/size/window_height_override=180
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Template"

[editor_plugins]

enabled=PackedStringArray("res://addons/awesome_custom_cursor/plugin.cfg", "res://addons/awesome_input_icons/plugin.cfg", "res://addons/awesome_scene_manager/plugin.cfg", "res://addons/console/plugin.cfg", "res://addons/debug_menu/plugin.cfg", "res://addons/gd-blender-3d-shortcuts/plugin.cfg", "res://addons/save_system/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

